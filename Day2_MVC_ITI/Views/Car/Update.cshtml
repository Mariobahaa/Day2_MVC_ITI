
@{
    ViewBag.Title = "Update";

    Day2_MVC_ITI.Car Car = (Day2_MVC_ITI.Car)ViewBag.car;

}

<h2>Update</h2>

<form method="post">
    <label for="num">Number</label>
    <input type="text" value=@Car.Num id="num" name="id" />
    <br />
    <label for="col">Color</label>
    <input type="text" value=@Car.Color id="col" name="color"/>
    <br />
    <label for="mod">Model</label>
    <input type="text" value=@Car.Model id="mod" name="model"/>
    <br />
    <label for="man">Manufacturer</label>
    <input type="text" value=@Car.Manufacturer id="man" name="manufacturer" />
    <br />
    <br />
    <br />
    <button type="submit" >Update</button>
</form>

